LokationsVerwaltung::~LokationsVerwaltung() {
	/**
	 * Alle Daten l&ouml;schen.
	 */
	for (map<int, Gebietslokation*>::iterator it = gebieteSet.begin();
			it != gebieteSet.end(); it++) {
		delete it->second;
	}
}

