#include "SuchenKlasse.h"

SuchenKlasse::SuchenKlasse(LokationsVerwaltung &lokV) {
	// TODO Auto-generated constructor stub

}

SuchenKlasse::~SuchenKlasse() {
	// TODO Auto-generated destructor stub
}

vector<Gebietslokation*> SuchenKlasse::suchId(int id) {
	struct {  //unnamed local struct
		 bool operator==(Gebietslokation const * lok, int const id){
			 return lok->getId() == id;
		 }
	} vergleich;
	SuchenKlasse::suchen((Gebietslokation*)vergleich(NULL,id));
}
