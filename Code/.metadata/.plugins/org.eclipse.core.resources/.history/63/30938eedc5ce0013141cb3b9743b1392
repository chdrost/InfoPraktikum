#include "Punktlokation.h"

Punktlokation::Punktlokation(vector<string> *zeile, Gebietslokation *areaReference) : Linearlokation(zeile,areaReference) {
	//TODO Linearreference
	try{
	this->netzKontenNummerVor = stoi(zeile->at(NETZKNOTEN1_NR))}catch(const std::invalid_argument &e){
		this->netzKontenNummerVor = 0;
	}
}

