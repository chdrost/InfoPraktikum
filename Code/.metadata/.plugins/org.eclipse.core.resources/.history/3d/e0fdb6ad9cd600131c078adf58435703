vector<Gebietslokation*> SuchenKlasse::suchUrban(bool urban) {
	struct Equals {
		bool operator()(Gebietslokation* lok, bool vergleich) {
			if (lok->getType() > GEBIET) {
				if (((Linearlokation*) lok)->isUrban();
						== vergleich) {
					return (true);
				} else {
					return (false);
				}
			} else {
				return (false);
			}
		}
	} vergleichsOperator;
	return (suche(vergleichsOperator, id));
}

